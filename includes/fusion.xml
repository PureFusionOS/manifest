<?xml version="1.0" encoding="UTF-8"?>
<manifest>

 <!-- PureFusionOS -->
 <project path="build/make" name="PureFusionOS/android_build_make" groups="pdk" remote="fusion" >
   <copyfile src="core/root.mk" dest="Makefile" />
   <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
   <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
   <linkfile src="core" dest="build/core" />
   <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
   <linkfile src="target" dest="build/target" />
   <linkfile src="tools" dest="build/tools" />
</project>
  <project path="build/kati" name="PureFusionOS/android_build_kati" groups="pdk,tradefed" remote="fusion" />
  <project path="build/soong" name="PureFusionOS/android_build_soong" groups="pdk,tradefed" remote="fusion" >
  <linkfile src="root.bp" dest="Android.bp" />
  <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
</project>
  <project path="frameworks/base" name="PureFusionOS/android_frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="fusion" />
  <project path="frameworks/support" name="PureFusionOS/android_frameworks_support" groups="pdk-cw-fs,pdk-fs" remote="fusion" />
  <project path="frameworks/av" name="PureFusionOS/android_frameworks_av" groups="pdk" remote="fusion" />
  <project path="manifest" name="PureFusionOS/manifest" remote="fusion" />
  <project path="vendor/pure" name="PureFusionOS/android_vendor_pure" remote="fusion" >
    <linkfile src="build.sh" dest="build.sh" />
  </project>

  <!-- Apps -->
 <project path="packages/apps/Settings" name="PureFusionOS/android_packages_apps_Settings" groups="pdk-fs" remote="fusion" />
 <project path="packages/apps/DUI" name="PureFusionOS/android_packages_apps_DUI" groups="pdk-fs" remote="fusion" />
</manifest>
